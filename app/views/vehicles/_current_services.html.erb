<strong>Services:</strong>
	<table class="table table-bordered">
	  <tr>
	  	<th>Type</th>
	  	<th>Service Date</th>
	  	<th>Service Mileage</th>
	  	<th>Service Due Mileage</th>
	  	<th>Due Date</th>
	  </tr>
		<% @vehicle.vehicle_services.each do |x| %>
		  <tr>
			  <td><%= x.service_type.name %></td>
			  <td><%= link_to x.check_for_default_date, edit_vehicle_service_path(x.id) %></td>
			  <td><%= link_to x.check_for_default_mileage, edit_vehicle_service_path(x.id) %></td>
			  <td><%= x.mileage_at_service + x.service_type.mileage_interval %></td>
			  <td><%= x.date_of_service + x.service_type.month_interval.months %></td>
		</tr>
		<% end %>
	</table>