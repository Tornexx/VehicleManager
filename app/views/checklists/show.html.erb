<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @checklist.name %>
</p>

<p>
  <strong>Comments:</strong>
  <%= @checklist.comments %>
</p>
<% @checklist.properties.each do |name, value| %>
	<p>
	  <b><%= name.humanize %>:</b>
	  <%= value %>
	</p>
<% end %>

<%= link_to 'Edit', edit_checklist_path(@checklist) %> |
<%= link_to 'Back', user_path(current_user.id) %>
