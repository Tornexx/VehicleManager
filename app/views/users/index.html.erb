<table class="table table-bordered">
  <thead>
    <tr>
      <th>Email</th>
      <th>Assigned Vehicles</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
		<td><%= assigned_vehicles(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

  